// Manifest file (manifest.json)
{
  "manifest_version": 3,
  "name": "My Privacy Extension",
  "version": "1.0",
  "description": "A simple privacy-focused browser extension",
  "permissions": ["storage", "webRequest", "webRequestBlocking"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}